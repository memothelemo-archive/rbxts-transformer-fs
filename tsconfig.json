    "module": "commonjs",
    "outDir": "out",
    "rootDir": "src",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "downlevelIteration": true,
    "types": ["@types/node"]
	},
  "exclude": [
    "old/**/**",
    "test/**/**"
  ]
}
